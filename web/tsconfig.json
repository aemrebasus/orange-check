/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@shared": [
        "src/app/shared/index"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@pages": [
        "src/app/pages/index"
      ],
      "@pages/*": [
        "src/app/pages/*"
      ],
      "@components": [
        "src/app/components/index"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@data": [
        "src/app/data/index"
      ],
      "@data/*": [
        "src/app/data/*"
      ],
      "@services": [
        "src/app/services/index"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@models": [
        "src/app/models/index"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@config": [
        "src/app/config/index"
      ],
      "@config/*": [
        "src/app/config/*"
      ],
      "@store": [
        "src/app/store/index"
      ],
      "@store/*": [
        "src/app/store/*"
      ]
    },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}